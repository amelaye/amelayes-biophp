{
    "name": "amelaye/amelayes-biophp",
    "authors": [
        {
            "name": "Amelie DUVERNET",
            "email": "amelieonline@gmail.com",
            "homepage": "http://www.amelionline.net",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0-only",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\Tests\\Behat\\Context\\": "features/context/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "ext-gd": "^7.1",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "csa/guzzle-bundle": "^3.1",
        "deployer/deployer": "^6.4",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/instantiator": "^1.2",
        "doctrine/orm": "^2.6",
        "jms/serializer-bundle": "^2.4",
        "phpstan/phpstan": "^0.11.19",
        "phpunit/phpunit": "^7.5",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/dom-crawler": "3.4.22",
        "symfony/monolog-bundle": "^3.3",
        "symfony/options-resolver": "^3.4",
        "symfony/symfony": "3.4.22",
        "twbs/bootstrap": "^4.3",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}
